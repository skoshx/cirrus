{
	"name": "cirrus2",
	"version": "0.4.0",
	"description": "Cirrus is a push to deploy tool written above the cirrus clouds. Inspired by Evan You's pod.",
	"repository": "skoshx/cirrus",
	"bin": {
		"cirrus": "./dist/cli.js",
		"cirrus2": "./dist/cli.js"
	},
	"main": "./dist/index.cjs",
	"type": "module",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.cjs"
	},
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"license": "MIT",
	"engines": {
		"node": ">=12.13"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"test": "NODE_NO_WARNINGS=1 ava --serial",
		"build": "rimraf dist && tsup src/cli.ts --format esm --dts",
		"build:lib": "rimraf dist && NODE_ENV=test tsup src/exports.ts --dts",
		"prepublishOnly": "npm run build",
		"prettier-check": "prettier --check .",
		"format": "prettier --write .",
		"type-check": "tsc --noEmit",
		"release": "np"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm",
			"--experimental-specifier-resolution=node"
		]
	},
	"keywords": [
		"push",
		"deploy",
		"deployment",
		"cirrus",
		"cloud",
		"typescript",
		"vps",
		"cli"
	],
	"devDependencies": {
		"@rollup/plugin-commonjs": "^21.0.3",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@types/dotenv": "^8.2.0",
		"@types/node": "^18.6.3",
		"@types/polka": "^0.5.4",
		"@types/react": "^17.0.39",
		"ava": "^4.3.1",
		"esbuild": "^0.14.18",
		"np": "^7.6.0",
		"polka": "^0.5.2",
		"prettier": "2.4.0",
		"rollup": "^2.67.0",
		"rollup-plugin-dts": "^4.1.0",
		"rollup-plugin-esbuild": "^4.8.2",
		"rollup-plugin-preserve-shebang": "^1.0.1",
		"ts-node": "^10.9.1",
		"tsup": "^6.2.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"cli-spinners": "^2.6.1",
		"dotenv": "^16.0.1",
		"execa": "^6.1.0",
		"ink": "^3.2.0",
		"logpile": "^1.0.6",
		"meow": "github:aaronccasanova/meow#381402f6744311cf6f0cd106424e5f432acde0c2",
		"ms": "3.0.0-canary.1",
		"pm2": "^5.2.0",
		"public-ip": "^5.0.0",
		"react": "^17.0.2",
		"rimraf": "^3.0.2",
		"zod": "^3.11.6"
	}
}
